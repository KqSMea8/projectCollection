    "dishes": "./src/entry/dishes.jsx",
    "emeberdialog": "./src/entry/emeberdialog.jsx",
    "boh-home": "./src/entry/boh-home.jsx",
    "bohauthiframe": "./src/entry/boh-auth-iframe.jsx",
    "bohauth": "./src/entry/boh-auth.jsx",
    "commonerror": "./src/entry/common-error.jsx",
    "boh-management": "./src/entry/boh-management.jsx",
    "ka-contract": "./src/entry/kacontract.jsx"
  },
  "dependencies": {
    "@ali/tracker": "^3.0.1",
    "@alipay/ajax": "^2.5.0",
    "@alipay/hermes-asynctree": "3.0.1-alpha.6",
    "@alipay/kb-ajax": "^1.0.4",
    "@alipay/kb-fetch": "^1.0.1",
    "@alipay/kb-systems-config": "^2.1.5",
    "@alipay/page-store": "^1.1.4",
    "@alipay/xform": "0.1.0-alpha.18",
    "antd": "~1.5.1",
    "async-validator": "^1.8.2",
    "babel-runtime": "^6.9.2",
    "classnames": "^2.2.1",
    "decimal.js": "^5.0.8",
    "gregorian-calendar": "^4.1.1",
    "gregorian-calendar-format": "^4.1.0",
    "hermes-react": "^1.4.1",
    "hermes-treeselect": "0.1.6-beta.9",
    "history": "^1.16.0",
    "immutable": "^3.8.1",
    "kb_postmsg": "^1.0.1",
    "layout": "^2.2.0",
    "lodash": "^4.11.1",
    "lodash.debounce": "^3.1.1",
    "matchmedia-polyfill": "^0.3.0",
    "moment": "^2.13.0",
    "object-assign": "^4.1.0",
    "prop-types": "^15.6.1",
    "qrcode.react": "^0.6.1",
    "rc-util": "^4.5.0",
    "react": "~0.14.x",
    "react-copy-to-clipboard": "^4.1.0",
